    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@eslint/js": "^9.21.0",
    "@parcel/babel-preset-env": "^2.13.3",
    "@testing-library/jest-dom": "^6.6.3",
    "babel-jest": "^29.7.0",
    "buffer": "^6.0.3",
    "eslint": "^8.57.1",
    "eslint-plugin-jest": "^28.11.0",
    "firebase-mock": "^2.3.2",
    "gh-pages": "^6.0.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.0.0",
    "parcel": "^2.13.3",
    "process": "^0.11.10"
  },
  "dependencies": {
    "firebase": "^10.14.1"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
